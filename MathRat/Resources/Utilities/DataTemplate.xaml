<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:MathRat.ViewModels"
                    xmlns:view="clr-namespace:MathRat.Views">
    <DataTemplate DataType="{ x:Type vm:HomeViewModel }">
        <view:Home />
    </DataTemplate>

    <DataTemplate DataType="{ x:Type vm:CustomerViewModel }">
        <view:Customers />
    </DataTemplate>

    <DataTemplate DataType="{ x:Type vm:ProductViewModel }">
        <view:Products />
    </DataTemplate>

    <DataTemplate DataType="{ x:Type vm:OrderViewModel }">
        <view:Orders />
    </DataTemplate>

    <DataTemplate DataType="{ x:Type vm:TransactionViewModel }">
        <view:Transactions />
    </DataTemplate>

    <DataTemplate DataType="{ x:Type vm:ShipmentViewModel }">
        <view:Shipments />
    </DataTemplate>

    <DataTemplate DataType="{ x:Type vm:SettingViewModel }">
        <view:Settings />
    </DataTemplate>
</ResourceDictionary>